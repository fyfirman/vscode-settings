  "editor.tokenColorCustomizations": {
    "[upper-ocean]": {
      "keywords": "#fce117",
      "textMateRules": [
        {
          "scope": ["entity.other.attribute-name", "keyword"],
          "settings": {
            "foreground": "#f2be7f"
          }
        },
        {
          "name": "Class, Support",
          "scope": [
            "entity.name",
            "support.type",
            "support.class",
            "support.other.namespace.use.php",
            "meta.use.php",
            "support.other.namespace.php",
            "markup.changed.git_gutter",
            "support.type.sys-types"
          ],
          "settings": {
            "foreground": "#fd61d9"
          }
        },
        {
          "name": "String, Symbols, Inherited Class, Markup Heading",
